{
    "name": "drupal/recommended-project",
    "description": "Project template for Drupal 8 projects with a relocated document root",
    "type": "project",
    "license": "GPL-2.0-or-later",
    "homepage": "https://www.drupal.org/project/drupal",
    "support": {
        "docs": "https://www.drupal.org/docs/user_guide/en/index.html",
        "chat": "https://www.drupal.org/node/314178"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        }
    ],
    "require": {
        "composer/installers": "^1.2",
        "drupal/bootstrap": "^3.21",
        "drupal/cas": "^1.6",
        "drupal/cas_attributes": "^1.0@beta",
        "drupal/core-composer-scaffold": "^8.9",
        "drupal/core-project-message": "^8.8",
        "drupal/core-recommended": "^8.9",
        "drupal/devel": "^2.1",
        "drupal/easy_email": "^1.0@beta",
        "drupal/environment_indicator": "^3.7",
        "drupal/extlink": "^1.3",
        "drupal/facets": "^1.4",
        "drupal/facets_block": "^1.3",
        "drupal/fakeobjects": "^1.0",
        "drupal/fontawesome": "^2.16",
        "drupal/honeypot": "^1.30",
        "drupal/imageapi_optimize": "^2.0@beta",
        "drupal/imageapi_optimize_binaries": "^1.0@alpha",
        "drupal/ldap": "^3.0@beta",
        "drupal/message_digest": "^1.0@RC",
        "drupal/node_view_permissions": "^1.2",
        "drupal/override_node_options": "^2.5",
        "drupal/purge": "^3.0",
        "drupal/queue_ui": "^2.1",
        "drupal/redirect": "^1.6",
        "drupal/redis": "^1.4",
        "drupal/samlauth": "^3.0",
        "drupal/schema_metatag": "^1.4",
        "drupal/search_api_solr": "^4.0",
        "drupal/simple_sitemap": "^3.6",
        "drupal/smart_read_more_link": "^1.7",
        "drupal/swiftmailer": "^1.0@beta",
        "drupal/twig_field_value": "^2.0",
        "drupal/twig_tweak": "^2.5",
        "drupal/upgrade_status": "^2.9",
        "drupal/varnish_purge": "^2.0",
        "drush/drush": "^10.2",
        "predis/predis": "^1.1",
        "symfony/event-dispatcher": "4.3.11 as 3.4.41",
        "umd_digital/umd_terp": "^8.1",
        "umd_digital/umd_terp_base": "^8.1"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true
    },
    "extra": {
        "drupal-scaffold": {
            "locations": {
                "web-root": "web/"
            }
        },
        "installer-paths": {
            "web/core": [
                "type:drupal-core"
            ],
            "web/libraries/{$name}": [
                "type:drupal-library"
            ],
            "web/modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "web/profiles/contrib/{$name}": [
                "type:drupal-profile"
            ],
            "web/themes/contrib/{$name}": [
                "type:drupal-theme"
            ],
            "drush/Commands/contrib/{$name}": [
                "type:drupal-drush"
            ],
            "web/modules/custom/{$name}": [
                "type:drupal-custom-module"
            ],
            "web/themes/custom/{$name}": [
                "type:drupal-custom-theme"
            ]
        },
        "drupal-core-project-message": {
            "include-keys": [
                "homepage",
                "support"
            ],
            "post-create-project-cmd-message": [
                "<bg=blue;fg=white>                                                         </>",
                "<bg=blue;fg=white>  Congratulations, youâ€™ve installed the Drupal codebase  </>",
                "<bg=blue;fg=white>  from the drupal/recommended-project template!          </>",
                "<bg=blue;fg=white>                                                         </>",
                "",
                "<bg=yellow;fg=black>Next steps</>:",
                "  * Install the site: https://www.drupal.org/docs/8/install",
                "  * Read the user guide: https://www.drupal.org/docs/user_guide/en/index.html",
                "  * Get support: https://www.drupal.org/support",
                "  * Get involved with the Drupal community:",
                "      https://www.drupal.org/getting-involved",
                "  * Remove the plugin that prints this message:",
                "      composer remove drupal/core-project-message"
            ]
        }
    }
}
