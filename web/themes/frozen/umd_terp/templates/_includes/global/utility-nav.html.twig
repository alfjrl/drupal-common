{#
/**
 * @file
 * Displays the utility navigation menu.
 *
 */
#}

<h2 class="visually-hidden" id="block-utilitynavigation-menu">Utility Navigation</h2>

<ul class="utility-menus">
  <li>
    <button id="u-hours-btn" class="btn-noborder">
      <span class="fas fa-clock"></span><br />
      <span class="utility-label utility-nav-locations">Locations &amp; Hours</span>
    </button>
  </li>
  <li>
    <button id="u-system-btn" class="btn-noborder">
      <span class="fas fa-exclamation-triangle"></span><br />
      <span class="utility-label utility-nav-systems-status">Systems Status</span>
    </button>
  </li>
  <li>
    <button id="u-help-btn" class="btn-noborder">
      <span class="fas fa-question-circle"></span><br />
      <span class="utility-label utility-nav-help">Get Help</span>
    </button>
  </li>
  <li>
    <button id="u-info-btn" class="btn-noborder">
      <span class="fas fa-info-circle"></span><br />
      <span class="utility-label utility-nav-info">Info For</span>
    </button>
  </li>
  <li>
    <button id="u-accounts-btn" class="btn-noborder">
      <span class="fas fa-user"></span><br />
      <span class="utility-label utility-nav-accounts">My Accounts</span>
    </button>
  </li>
</ul>
